//Write a program to print the following pattern:

#include <stdio.h>

int main() {
    int i, j;

    for (i = 5; i >= 1; i--) {
        
        for (j = i; j <= 5; j++) {
            printf("%d", j); 
        }
        printf("\n"); 
    }

    return 0; 
}

//Write a program to print the following pattern:

#include <stdio.h>

int main() {
    int rows = 5; 
    int i, j, space;

    for (i = 1; i <= rows; i++) {
        for (space = 1; space < i; space++) {
            printf(" ");
        }
       
        for (j = 1; j <= rows - i + 1; j++) {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
